    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^4.0.0",
    "helmet-csp": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.3.2",
    "morgan": "^1.10.0",
    "nodemon": "^1.19.1",
